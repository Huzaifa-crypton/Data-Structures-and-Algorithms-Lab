    "num1 = int(input(\"ENTER FACTORIAL NUMBER: \"))\n",
    "def factorial(num):\n",
    "    if(num == 0):\n",
    "        return 1\n",
    "    else:\n",
    "        return num*factorial(num-1)\n",
    "print(factorial(num1))    \n",
    "    "
